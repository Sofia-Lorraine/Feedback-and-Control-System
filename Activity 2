import control as ctrl

num = [10,20]
den = [1,15,20,0]

import control as ctrl
import matplotlib.pyplot as plt

G = ctrl.TransferFunction(num, den)
print(G)
t, y =ctrl.step_response(G)
plt.title("Graph")
plt.plot(t, y)
plt.xlabel('Time (s)')
plt.ylabel('output')
plt.grid()
plt.show()
